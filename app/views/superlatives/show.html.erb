<h2><%= @superlative.name %></h2>

<h3>Winner is <%= @superlative.winner.first.name %></h3>

<% @launchers.each do |launcher| %>

  <%= simple_form_for [@superlative, @vote] do |f| %>
    <%= f.hidden_field :launcher, value:launcher.id %>
    <%= f.button :submit, "+1" %>
  <% end %>

  <%= launcher.name %><br><br>

<% end %>
